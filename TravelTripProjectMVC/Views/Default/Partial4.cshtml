@using TravelTripProjectMVC.Models.Classes
@model List<Blog>
<div class="padding1">
    <h4>Favori Yerlerimiz</h4>
    <div class="box-left">
        @foreach (var item in Model)
        {
            <div class="bot-img">
                <img src="@item.BlogImagePath" style="width:120px; height:65px;" alt="" />
            </div>
            <div class="bot-desc">
                <p>@item.Aciklama.Substring(0,65)... </p>
            </div>
            <div class="clear"></div>
            
        }

    </div>
    @Html.Action("Partial5", "Default")
</div>

